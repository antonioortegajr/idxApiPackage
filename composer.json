{
    "name": "idxBrokerApi",
    "description": "package for calling the IDX Broker API",
    "keywords": [
        "IDX",
        "IDX Broker",
        "real estate"
    ],
    "homepage": "https://github.com/antonioortegajr/idxApiPackage",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Orteag Jr",
            "email": "antonioorteagjr@gmail.com",
            "homepage": "http://antonioorteagjr.com",
            "role": "Developer"
        }
    ],
    "require": {
      "php" : ">=5.5.0",
      "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "League\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Skeleton\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
